<h1><%= @building.name %></h1>

<h3> Total Rent Income </h3>
<p><%=@building.total_rent%></p>
<h3> Offices Available on these floors: </h3>
<%@building.empty_offices.each do |office| %>
<p><%= office.floor%> </p>
<%end%>
<h3>Companies:</h3>
<p><% @building.companies.each do |company| %> </p>
<p> <%=link_to company.name, company_path(company)%> </p>




<%end%>

